{
    "name": "zeichen32/gitlabapibundle",
    "description": "Symfony Bundle to include the gitlab api.",
    "type": "symfony-bundle",
    "keywords": ["gitlab", "api"],
    "homepage": "https://github.com/Zeichen32/GitLabApiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Averkamp",
            "email": "j.averkamp@two-developers.com"
        }
    ],
    "require": {
        "php": "^7.2||^8.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^1.0|^2.0",
        "nyholm/psr7": "^1.0",
        "m4tthumphrey/php-gitlab-api": "^11.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "autoload": {
        "psr-0": { "Zeichen32\\GitLabApiBundle": "" }
    },
    "target-dir": "Zeichen32/GitLabApiBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
