{
    "name": "zeichen32/gitlabapibundle",
    "description": "Symfony2 Bundle to include the gitlab api.",
    "type": "symfony-bundle",
    "keywords": ["gitlab", "api"],
    "homepage": "https://github.com/Zeichen32/GitLabApiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Averkamp",
            "email": "j.averkamp@two-developers.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "m4tthumphrey/php-gitlab-api": "^9"
    },
    "require-dev" : {
        "php-http/message": "^1.6",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-0": { "Zeichen32\\GitLabApiBundle": "" }
    },
    "target-dir": "Zeichen32/GitLabApiBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
